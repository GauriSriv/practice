#include<iostream>
using namespace std;
int search(int a[],int n, int key)
{
    for(int i=0;i<n;i++)
    {
        if(a[i]==key)
        {
            return i;
        }
    }
            return n;
        
    }


int main()
{
    int a[]={2,4,5,6,10,19};
    int n=6;
    int key=1;
    
    cout<<search(a,n,key);
    return 0;
}
