#include<iostream>
#include<cstring>
using namespace std;
int sti(char *a,int n)
{
    if(n==0)
    {
        return 0 ;}
        int digit=a[n-1]-'0';   //recursive
        return sti(a,n-1)*10+digit;
}
int main()
{
    char a[]="23456";
    int n=strlen(a);
    int ans=sti(a,n);
    cout<<ans<<endl;
    cout<<ans+1<<endl;
    return 0;
}
