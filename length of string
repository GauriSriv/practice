
#include<iostream>
using namespace std;
int main()
{ int count =0;
    char a[]="Gauri Srivastava";
    cout<<a<<endl;
    for(int i=0;a[i]!='\0';i++)
    {
        cout<<a[i];
        count++;
    }
    cout<<endl;
    cout<<"total no of string is:"<<count;
}
//we can use the following to get length directly 
//cout<<strlen(a)<<endl;
