#include<iostream>
using namespace std;

void readline(char a[],char del)
{
    int i=0;
    char ch= cin.get();
    
    while(ch!=del)
    {
        a[i]=ch;
        i++;
        ch=cin.get();
    }
    a[i]='\0';
}
int main()
{
    char a[10];
    
    readline(a,'@');
    cout<<a<<endl;
    return 0;
}
